/*
C++ Program to print the following star pattern.
Input : 4 and 6
Output :
******
******
******
******
*/

#include <iostream>
using namespace std;
int main()
{
	int row,column;
	cout<<"Enter number of rows : ";
	cin>>row;
	cout<<"Enter number of columns : ";
	cin>>column;
	int i = 1;
	while(i<=row)
	{
		int j = 1;
		while(j<=column)
		{
			cout<<"*";
			j++;
		}
		cout<<endl;
		i++;
	}
}
